{
  "request": {
    "method": "POST",
    "urlPath": "/EDCDiscovery/administration/connectors/discovery"
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": [
      {
        "bpn": "{{jsonPath request.body '$.[0]'}}",
        "connectorEndpoint": [
          "http://localhost:8082/edc"
        ]
      }
    ],
    "transformers": [
      "response-template"
    ]
  }
}
